{
  "name": "astral-draft",
  "version": "1.0.1",
  "type": "module",
  "description": "Elite Fantasy Football",
  "main": "index.tsx",
  "scripts": {
    "prestart": "node scripts/startup-check.js",
    "dev": "vite --port 8765",
    "dev:oracle": "vite --port 8765 --open",
    "build": "vite build --mode production",
    "build:prod": "vite build --mode production",
    "build:verify": "npm run build && node scripts/verify-production-build.cjs",
    "build:analyze": "vite build --mode analyze && npx rollup-plugin-visualizer dist --open",
    "preview": "vite preview",
    "start": "vite",
    "server": "npx ts-node --project tsconfig.backend.json backend/server.ts",
    "server:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/server.ts\"",
    "server:prod": "NODE_ENV=production npx ts-node --project tsconfig.backend.json backend/server.ts",
    "server:db": "npx ts-node --project tsconfig.backend.json backend/server-with-db.ts",
    "server:db:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/server-with-db.ts\"",
    "oracle:server": "npx ts-node --project tsconfig.backend.json backend/oracleServer.ts",
    "oracle:dev": "npx nodemon --exec \"npx ts-node --project tsconfig.backend.json backend/oracleServer.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:accessibility": "jest --config=jest.accessibility.config.js",
    "test:accessibility:watch": "jest --config=jest.accessibility.config.js --watch",
    "test:accessibility:ci": "jest --config=jest.accessibility.config.js --ci --coverage --watchAll=false",
    "accessibility:test": "node scripts/runAccessibilityTests.js",
    "accessibility:test:ci": "node scripts/runAccessibilityTests.js --fail-on-violations=true",
    "accessibility:test:local": "node scripts/runAccessibilityTests.js --local",
    "accessibility:ci": "node scripts/ciAccessibilityTester.js",
    "accessibility:ci:validate": "node scripts/ciAccessibilityTester.js --validate-only",
    "accessibility:dashboard": "node scripts/accessibilityDashboardIntegration.js",
    "accessibility:dashboard:dev": "node scripts/accessibilityDashboardIntegration.js && npm run dev",
    "accessibility:monitor": "node scripts/accessibilityDashboardIntegration.js --monitor",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "npm run build:prod && netlify deploy --prod",
    "deploy:docker": "docker-compose up -d --build",
    "deploy:production": "chmod +x scripts/deployment/deploy.sh && scripts/deployment/deploy.sh deploy",
    "deploy:staging": "docker-compose -f docker-compose.staging.yml up -d --build",
    "health:check": "curl -f http://localhost:3001/health || exit 1",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "logs:all": "docker-compose logs -f",
    "backup:db": "docker-compose exec backend sqlite3 /app/data/astral-draft.db '.backup /app/backups/manual-backup.db'",
    "restore:db": "docker-compose exec backend sqlite3 /app/data/astral-draft.db '.restore /app/backups/manual-backup.db'",
    "ssl:setup": "chmod +x scripts/deployment/setup-ssl.sh && sudo scripts/deployment/setup-ssl.sh production",
    "ssl:renew": "sudo certbot renew && docker-compose restart nginx",
    "perf:audit": "npm run build:analyze && node scripts/performance/performanceAudit.cjs",
    "perf:monitor": "node scripts/performance/performanceMonitor.cjs",
    "perf:optimize": "node scripts/performance/bundleOptimizer.cjs",
    "perf:report": "npm run build:analyze && node scripts/performance/generatePerformanceReport.cjs",
    "bundle:size": "npm run build && bundlesize",
    "bundle:visualize": "npm run build:analyze",
    "cache:optimize": "node scripts/performance/cacheOptimizer.cjs",
    "image:optimize": "node scripts/performance/imageOptimizer.cjs",
    "setup:database": "node setup-neon-database.cjs",
    "db:setup": "node setup-neon-database.cjs",
    "setup": "node setup.js",
    "start:backend": "cd server && npm run dev",
    "start:full": "concurrently \\"npm run start:backend\\" \\"npm run dev\\"",`n    "dev:full": "node start-development.js",`n    "start:dev": "node start-development.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@google/genai": "^1.9.0",
    "@neondatabase/serverless": "^1.0.1",
    "@netlify/functions": "^4.2.1",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/csrf": "^1.3.2",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/validator": "^13.15.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "chart.js": "^4.5.0",
    "compression": "^1.8.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.3",
    "helmet": "^7.2.0",
    "idb": "^8.0.3",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.535.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.4.0",
    "validator": "^13.15.15",
    "why-is-node-running": "^3.2.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "axios-cookiejar-support": "^6.0.4",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "crypto-browserify": "^3.12.1",
    "cssnano": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "stream-browserify": "^3.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "tough-cookie": "^5.1.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "util": "^0.12.5",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "whatwg-fetch": "^3.6.20",
    "workbox-precaching": "^7.3.0"
  }
}
