# Astral Draft Environment Variables Template
# Copy this file to .env and fill in your actual values

# Sports Data API Key (get from SportsIO)
VITE_SPORTS_IO_API_KEY=your_sports_io_api_key_here

# Google Gemini API Key (for AI features)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Environment Settings
NODE_ENV=development
VITE_NODE_ENV=development

# WebSocket URL for real-time features
VITE_WS_URL=http://localhost:3001

# Optional: Additional API endpoints
VITE_API_BASE_URL=http://localhost:3001/api
VITE_ENABLE_ORACLE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true

# Database URL (if using external database)
VITE_DATABASE_URL=your_database_url_here

# Stripe Keys (if using payments)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key_here